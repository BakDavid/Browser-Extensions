{
    "manifest_version": 3,
    "name": "Dirty JSON Cleaner",
    "version": "1.0",
    "description": "Extension that cleans wrongly formatted json bodies. Fixes problems with \n mainly",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}
